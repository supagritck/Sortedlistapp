        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SortedListApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="600">
    <Grid Margin="0,0,2,3">
        <Button x:Name="Addbtn" Content="Add" HorizontalAlignment="Left" Height="62" Margin="380,113,-337,-121" VerticalAlignment="Top" Width="136" Click="Addbtn_Click"/>
        <TextBox x:Name="Keytxt" HorizontalAlignment="Left" Height="24" Margin="175,113,-185,-83" TextWrapping="Wrap" VerticalAlignment="Top" Width="189"/>
        <TextBox x:Name="Valuetxt" HorizontalAlignment="Left" Height="24" Margin="175,151,-185,-121" TextWrapping="Wrap" VerticalAlignment="Top" Width="189"/>
        <Label Content="Key" HorizontalAlignment="Left" Height="27" Margin="109,110,0,-83" VerticalAlignment="Top" FontSize="14"/>
        <Label Content="Value" HorizontalAlignment="Left" Height="27" Margin="102,148,0,-121" VerticalAlignment="Top" FontSize="14"/>

    </Grid>
</Window>
